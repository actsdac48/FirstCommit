package transaction;


import java.time.LocalDate;
import java.time.LocalDateTime;
import java.util.ArrayList;

public class Entry {

	public static void main(String[] args) {
		LocalDateTime d = LocalDateTime.now();
		Transaction ob1 = new Transaction(1, d, 200.0f, true, true);
		Transaction ob2 = new Transaction(2, d, 2000.0f, true, true);
				
		ArrayList<Transaction> trnsCollecton = new ArrayList<Transaction>();
		trnsCollecton.add(ob1);
		trnsCollecton.add(ob2);
		
		for(Transaction data : trnsCollecton)
		{
			System.out.println(data);
		}
		

	}

}

package classcode;

public class Entry {

	public static void main(String[] args) {
		
		Increament incThread = new Increament();
		
		incThread.start();
		incThread.setDaemon(true);
		
		Decrement decThread = new Decrement();
		Thread threadObj = new Thread(decThread);
		
		threadObj.start();
		
		
		System.out.println("IN MAIN***********************************************");
	}

}

package classcode;

public class Decrement implements Runnable {

	@Override
	public void run() {


		while(true) {
			try {
				System.out.println("Run in Decrement----------");
			
				Thread.sleep(1000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
		
	}

}
